#:kivy 1.0.9
        
<PlotTest>:
    BoxLayout:
        orientation: 'vertical' 
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            BoxLayout: 
                orientation: 'vertical' 
                size_hint_y: None
                size_hint_x: 1
                size: self.width, 600
                cols: 3
                id: destination
        Button: 
            text: 'OK'
            size_hint: 0.3, 0.08
            pos_hint: {'x': 0.35, 'top':0.1}
            on_release: root.dismiss()